{
  "name": "to-do-app-be",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "outDir": "dist"
  },
  "include": [
    "src/**/*",
    "config/**/*"
  ],
  "files": [
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "scripts": {
    "postinstall": "npm run build-ts && rm node_modules/mongoose/node_modules/mongodb/mongodb.d.ts",
    "start": "npm run serve",
    "serve": "node dist/src/server.js",
    "watch-node": "nodemon dist/src/server.js",
    "build": "tsc",
    "build-ts": "tsc",
    "clean": "tsc --build --clean",
    "watch-ts": "tsc --watch"
  },
  "engines": {
    "node": "16.13.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^9.4.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.0",
    "mongoose": "^6.1.6",
    "underscore": "^1.13.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.8",
    "@types/underscore": "^1.11.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
